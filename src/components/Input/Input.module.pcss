.root {
}

.input {
  width: 100%;
  height: 48px;
  padding: 16px;
  border-radius: 100px;
  color: var(--surface-dark);
  border: 1px solid var(--surface-dark);
  background-color: transparent;

  &:focus {
    outline: none;
    color: var(--primary-text);
    border-color: var(--primary-main);
  }
}
